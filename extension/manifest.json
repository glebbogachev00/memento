{
  "manifest_version": 3,
  "name": "Memento",
  "description": "Memento tracks your life in weeks and productive time right in each new tab.",
  "version": "1.0.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_title": "Memento"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "permissions": [
    "storage"
  ]
}
